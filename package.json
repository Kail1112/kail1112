{
  "name": "kail1112",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://github.com/Kail1112/kail1112#readme",
  "bugs": {
    "url": "https://github.com/Kail1112/kail1112/issues"
  },
  "license": "MIT",
  "author": "Mikhail Ifutin",
  "type": "module",
  "bin": {
    "build-package": "./build/build-package/index.js",
    "create-package": "./build/create-package/index.js"
  },
  "scripts": {
    "bin:clean": "shx rm -rf build",
    "bin:compile": "swc src --strip-leading-paths -d build",
    "eslint": "eslint .",
    "eslint:check": "pnpm eslint",
    "eslint:fix": "pnpm eslint --fix",
    "postinstall": "pnpm bin:clean && pnpm bin:compile",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --max-warnings 0",
    "*.json": "prettier --write"
  },
  "devDependencies": {
    "@kail1112/eslint-configs": "workspace:^",
    "@swc/cli": "0.7.9",
    "@swc/core": "1.15.8",
    "@types/glob-parent": "5.1.3",
    "@types/node": "catalog:",
    "@types/yargs": "17.0.35",
    "dotenv": "17.2.3",
    "dotenv-expand": "12.0.3",
    "eslint": "catalog:",
    "glob": "13.0.0",
    "glob-parent": "5.1.2",
    "globals": "catalog:",
    "husky": "9.1.7",
    "jiti": "2.6.1",
    "kail1112": "workspace:^",
    "lint-staged": "16.2.7",
    "prettier": "catalog:",
    "prettier-plugin-packagejson": "2.5.20",
    "prettier-plugin-sort-json": "4.1.1",
    "shx": "0.4.0",
    "typescript": "catalog:",
    "yaml": "2.8.2",
    "yargs": "18.0.0"
  },
  "packageManager": "pnpm@10.27.0",
  "engines": {
    "node": ">=20",
    "npm": ">=10",
    "pnpm": ">=10"
  }
}
